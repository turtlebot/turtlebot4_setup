<?xml version="1.0" encoding="UTF-8"?>
<dds xmlns="http://www.eprosima.com/XMLSchemas/fastRTPS_Profiles">
  <profiles>
    <transport_descriptors>
      <transport_descriptor>
        <transport_id>udp_transport</transport_id>
        <type>UDPv4</type>
        <interfaces>
          <!--
            Block traffic to the create3;
            If Jazzy nodes contact the create3 directly the nodes on the
            create3 will crash because of DDS changes between Humble & Iron
            (and the create3 firmware is built for Humble)
          -->
          <blocklist>
            <interface name="usb0" />
            <interface name="192.168.186.0" netmask_filter="ON" />
          </blocklist>
        </interfaces>
      </transport_descriptor>
    </transport_descriptors>

    <participant profile_name="domainparticipant_profile_name" is_default_profile="true">
      <rtps>
        <userTransports>
          <transport_id>udp_transport</transport_id>
        </userTransports>
        <useBuiltinTransports>false</useBuiltinTransports>
      </rtps>
    </participant>
  </profiles>
</dds>
